<div id="<%= dom_id bibliographic_reference %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Reference type:</strong>
    <%= bibliographic_reference.reference_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Authors:</strong>
    <%= bibliographic_reference.authors %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Year:</strong>
    <%= bibliographic_reference.year %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= bibliographic_reference.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Publisher:</strong>
    <%= bibliographic_reference.publisher %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Publication location:</strong>
    <%= bibliographic_reference.publication_location %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Doi:</strong>
    <%= bibliographic_reference.doi %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Language:</strong>
    <%= bibliographic_reference.language %>
  </div>
</div>
